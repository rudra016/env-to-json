# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp
DB_USER=admin
DB_PASSWORD=super_secret_password
DB_SSL=true

# Application Settings
APP_NAME=My Awesome App
APP_VERSION=1.0.0
APP_ENV=development
APP_PORT=3000
APP_DEBUG=true

# API Keys and Secrets
API_KEY=abc123def456ghi789
JWT_SECRET=my_jwt_secret_key_here
ENCRYPTION_KEY=very_secret_encryption_key

# Third Party Services
REDIS_URL=redis://localhost:6379
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=noreply@example.com
SMTP_PASSWORD=email_password_here

# React App Variables
REACT_APP_API_URL=https://api.example.com
REACT_APP_VERSION=1.0.0
REACT_APP_TITLE=My React App

# Feature Flags
FEATURE_NEW_UI=true
FEATURE_ANALYTICS=false
FEATURE_EXPERIMENTAL="beta testing"

# URLs with special characters
BASE_URL="https://example.com/api/v1"
CALLBACK_URL='https://oauth.example.com/callback'

# Multiline and escaped values
DESCRIPTION="This is a long description\nwith multiple lines\nand special chars: !@#$%"
JSON_CONFIG='{"timeout": 5000, "retries": 3}'